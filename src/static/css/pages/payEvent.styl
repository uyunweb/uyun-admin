table.sx_table {
	min-width: 100%;
	*width: 100%;
}
table.sx_table thead {
	background: #edf0f7;
}
table.sx_table th {
	font-weight:bold;
}
table.sx_table thead td {
	font-weight:bold;
}
table.sx_table,
table.sx_table tr th,
table.sx_table tr td {
	border: 1px solid #dde1e8;
}

table.sx_table th {
	padding: 6px 15px;
	text-align:center;
	white-space: nowrap;
	line-height: 28px;
	color: #1f2d3d;
}
table.sx_table th.gleft {
	text-align: left;
}
table.sx_table tr {
}
table.sx_table tbody tr:hover {
	background: #e5f3ff;
}
table.sx_table tr td {
	line-height:26px;
	padding:4px 2px;
	color:#484848;
	position: relative;
	/*white-space: nowrap;*/
}
table.sx_table tr td.no_data {
	height: auto;
	padding: 120px 0;
	text-align: center;
	color: #c4c6cd;
	background: #f1f4f7;
	line-height: 28px;
}
table.sx_table tr td.no_data i.iconfont {
	font-size: 70px;
}

.edit_input {
	/*box-sizing: border-box;*/
	/*position: absolute;*/
	top: -1px;
	left: -1px;
	padding: 1px;
	width: 100%;
	height: 100%;
	border: 0;
	background: none;
	text-align: center;
	z-index: 10;
	color: #484848;
	min-height: 32px;
}
.edit_input:focus {
	padding: 0;
	border: 1px solid #4db3ff;
	color: #000;
}

